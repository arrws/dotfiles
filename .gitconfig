[user]
	email = andreea@ieval.ro
	name = arrws

[credential]
	helper = store

[branch]
	autosetuprebase = always

[pull]
	rebase = false

[core]
    pager = delta --features=xtheme

[interactive]
    diffFilter = delta --features=xtheme

[delta "xtheme"]
    line-numbers=true
    line-numbers-left-format="{nm:>2}:"
    line-numbers-right-format="{np:>2}|"
    line-numbers-left-style="black"
    line-numbers-right-style="black"
    line-numbers-minus-style="#990000"
    line-numbers-zero-style="#666666"
    line-numbers-plus-style="#008800"
    hunk-header-style="omit"
    file-decoration-style=""
    file-style="brightblue"
    hyperlinks=true
    plus-non-emph-style=""
    plus-emph-style="syntax #004400"
    plus-style="syntax #002200"
    minus-non-emph-style=""
    minus-emph-style="syntax #660000"
    minus-style="syntax #440000"
    syntax-theme="xtheme"
